{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "cache": {
        "disabled": true
      },
      "env": {
        "EXPO_NO_CACHE": "1",
        "EXPO_SKIP_CACHE": "1",
        "CI": "1",
        "EXPO_IMAGE_UTILS_CACHE_DISABLED": "1",
        "EXPO_SKIP_ICON_GENERATION": "1",
        "EXPO_SKIP_SPLASH_GENERATION": "1",
        "EXPO_SKIP_IMAGE_PROCESSING": "1",
        "EXPO_DISABLE_IMAGE_UTILS": "1",
        "EXPO_SKIP_ASSET_GENERATION": "1"
      }
    },
    "preview": {
      "distribution": "internal",
      "cache": {
        "disabled": true
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "EXPO_NO_CACHE": "1",
        "EXPO_SKIP_CACHE": "1",
        "CI": "1",
        "EXPO_IMAGE_UTILS_CACHE_DISABLED": "1",
        "EXPO_SKIP_ICON_GENERATION": "1",
        "EXPO_SKIP_SPLASH_GENERATION": "1",
        "EXPO_SKIP_IMAGE_PROCESSING": "1",
        "EXPO_DISABLE_IMAGE_UTILS": "1",
        "EXPO_SKIP_ASSET_GENERATION": "1"
      }
    },
    "production": {
      "autoIncrement": true,
      "cache": {
        "disabled": true
      },
      "ios": {
        "distribution": "store",
        "image": "latest"
      },
      "env": {
        "EXPO_NO_CACHE": "1",
        "EXPO_SKIP_CACHE": "1",
        "CI": "1",
        "EXPO_IMAGE_UTILS_CACHE_DISABLED": "1",
        "EXPO_SKIP_ICON_GENERATION": "1",
        "EXPO_SKIP_SPLASH_GENERATION": "1",
        "EXPO_SKIP_IMAGE_PROCESSING": "1",
        "EXPO_DISABLE_IMAGE_UTILS": "1",
        "EXPO_SKIP_ASSET_GENERATION": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
