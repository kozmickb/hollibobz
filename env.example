# iOS/Android Configuration
IOS_BUNDLE_ID=com.appeningnow.odysync
ANDROID_PACKAGE=com.appeningnow.odysync

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/odysync
